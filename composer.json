{
    "name": "keboola/syrup",
    "license": "MIT",
    "type": "project",
    "description": "Syrup",
    "authors": [
        {
            "name": "Miroslav Čillík",
            "email": "miro@keboola.com"
        },
        {
            "name": "Jakub Matějka",
            "email": "jakub@keboola.com"
        }
    ],
    "autoload": {
        "psr-4": { "": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "": "tests/" }
    },
    "repositories": [],
    "require": {
        "php": "~5.6",
        "keboola/csv": "~1.1",
        "keboola/php-encryption": "~0.2",
        "keboola/storage-api-client": "^4.0",
        "keboola/php-temp": "~0.1.0",
        "symfony/symfony": "^3.1.0",
        "symfony/monolog-bundle": "^2.8",
        "symfony/assetic-bundle": "^2.3",
        "symfony/swiftmailer-bundle": "^2.3",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "sensio/generator-bundle": "^3.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-migrations-bundle": "^1.1",
        "aws/aws-sdk-php": "~3.2",
        "elasticsearch/elasticsearch": "~1.3",
        "defuse/php-encryption": "~1.2.1",
        "keboola/php-utils": "~1.0.0"
    },
    "require-dev": {
        "symfony/var-dumper": "~2.6",
        "phpunit/phpunit": "^5.0",
        "squizlabs/php_codesniffer": "2.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
            "bash git/setup.sh"
        ],
        "post-update-cmd": []
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
